Account page
<div data-vm="robot">
    <div class="crudl">
        <div class="crudl___controls">
            <h2>{{_ 'My Robots'}}</h2>
            <a v-on:click="create()">{{_ 'Add'}}</a>
            <a v-on:click="importer()">{{_ 'Import'}}</a>
        </div>
        <table>
            <thead>
                <tr>
                    <td>{{_ 'Robot Name'}}</td>
                    <td>{{_ 'Speed'}}</td>
                    <td>{{_ 'Weight'}}</td>
                    <td>{{_ 'Power'}}</td>
                </tr>
            </thead>
            <tbody>
                <tr v-for="robot in robots">
                    <td>
                        ${robot.name}
                    </td>
                    <td>
                        <a v-on:click="update(robot.id)">{{_ 'Edit'}}</a>
                        <a v-on:click="destroy(robot.id)">{{_ 'Delete'}}</a>
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
</div>
<script type="text/javascript" src="/js/viewmodels.js"></script>
